<% if signed_in? %>
  <%= link_to "Logout", destroy_user_session_path, method: :delete, class: "btn-logout #{class_private(style_nav)}" %>
<% else %>
  <%= link_to "Register", new_user_registration_path, class: "btn-register #{class_private(style_nav)}" %>
  <%= link_to "Login", new_user_session_path, class: "btn-login #{class_private(style_nav)}" %>
<% end %>
